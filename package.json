{
  "name": "@craigmiller160/react-keycloak",
  "version": "1.3.0-beta",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://nexus-craigmiller160.ddns.net/repository/npm-private/"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
