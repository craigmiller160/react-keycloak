    "@craigmiller160/eslint-config-react": "1.1.0",
    "@craigmiller160/eslint-config-ts": "^1.1.2",
    "@craigmiller160/prettier-config": "^1.0.2",
    "@craigmiller160/swc-config": "1.0.0-beta.9",
    "@swc/cli": "^0.1.62",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3",
    "vitest": "^0.28.3",
    "@vitejs/plugin-react-swc": "^3.2.0"
  },
  "dependencies": {
    "keycloak-js": "^20.0.3"
  }
